/ Contact Form
#modal-contact.tp--contact.text-center.white-popup.mfp-hide
  .page-header
    %h3 Get in touch
    %p Feel free to get in touch with us by sending your message below.
  %form#contact-form{:action => "contact-form/contact.php", :method => "post", :role => "form"}
    .messages
    .controls
      .row
        .col-md-12
          %label{:for => "form_name"} Full Name *
          .row
            .col-md-6
              .form-group
                %label.sr-only{:for => "form_lastname"} First Name *
                %input#form_name.form-control{:name => "name", :placeholder => "First name", :required => "required", :type => "text"}/
                .help-block.with-errors
            .col-md-6
              .form-group
                %label.sr-only{:for => "form_lastname"} Last Name *
                %input#form_lastname.form-control{:name => "surname", :placeholder => "Last name", :required => "required", :type => "text"}/
                .help-block.with-errors
      .row
        .col-md-12
          .form-group
            %label{:for => "form_email"} Email *
            %input#form_email.form-control{:name => "email", :placeholder => "Please enter your email", :required => "required", :type => "email"}/
            .help-block.with-errors
      .row
        .col-md-12
          .form-group
            %label{:for => "form_message"} Message *
            %textarea#form_message.form-control{:name => "message", :placeholder => "Please enter your message", :required => "required", :rows => "4"}
            .help-block.with-errors
        .col-md-12.text-center
          %input.btn.btn-primary.btn-lg.btn-circular.btn-send{:type => "submit", :value => "Send your message"}/

